<Project>
    <PropertyGroup>
        <Product>XAML Map Control</Product>
        <Authors>Clemens Fischer</Authors>
        <Copyright>Copyright Â© 2025 Clemens Fischer</Copyright>
        <Version>14.0.0</Version>
        <AssemblyVersion>$(Version)</AssemblyVersion>
